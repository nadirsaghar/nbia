<ns1:CQLQuery xmlns:ns1="http://CQL.caBIG/1/gov.nih.nci.cagrid.CQLQuery">
 <ns1:Target name="gov.nih.nci.ncia.domain.Patient">
  <ns1:Association name="gov.nih.nci.ncia.domain.Study"
roleName="studyCollection">
   <ns1:Association name="gov.nih.nci.ncia.domain.Series"
roleName="seriesCollection">
    <ns1:Group logicRelation="OR">
     <ns1:Attribute name="instanceUID" predicate="LIKE"
value="%Lung%"/>
     <ns1:Attribute name="bodyPartExamined" predicate="LIKE"
value="%lung%"/>
     <ns1:Attribute name="modality" predicate="LIKE" value="%Lung%"/>
    </ns1:Group>
   </ns1:Association>
  </ns1:Association>
 </ns1:Target>
</ns1:CQLQuery>
